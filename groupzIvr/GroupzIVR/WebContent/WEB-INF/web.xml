<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>GroupzIVR</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description>for Ivr Service Requests</description>
    <servlet-name>ServiceRequest</servlet-name>
    <servlet-class>ivr.servlets.ServiceRequest</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServiceRequest</servlet-name>
    <url-pattern>/ServiceRequest</url-pattern>
  </servlet-mapping>
  <servlet>
    <description>for Maintenance</description>
    <servlet-name>Maintenance</servlet-name>
    <servlet-class>ivr.servlets.SetMaintenance</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Maintenance</servlet-name>
    <url-pattern>/Maintenance</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ServiceEnquiry</display-name>
    <servlet-name>ServiceEnquiry</servlet-name>
    <servlet-class>ivr.servlets.ServiceEnquiry</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServiceEnquiry</servlet-name>
    <url-pattern>/ServiceEnquiry</url-pattern>
  </servlet-mapping>
</web-app>