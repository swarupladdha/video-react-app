<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>SNI</display-name>
  <welcome-file-list>
    <welcome-file>NewFile.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>ConnectionServlet</display-name>
    <servlet-name>ConnectionServlet</servlet-name>
    <servlet-class>com.sni.servlets.ConnectionServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>ConnectionServlet</servlet-name>
    <url-pattern>/connection</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>OFDServlet</display-name>
    <servlet-name>OFDServlet</servlet-name>
    <servlet-class>com.sni.servlets.GoogleServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>OFDServlet</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>FacebookServlet</display-name>
    <servlet-name>FacebookServlet</servlet-name>
    <servlet-class>com.sni.servlets.FacebookServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FacebookServlet</servlet-name>
    <url-pattern>/FacebookServlet</url-pattern>
  </servlet-mapping>
</web-app>